<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
    </startup>
  <appSettings>
    <!-- These IdentityServer settings will be the same for all members and users -->
    <add key="IdentityServer_ClientId" value="FILL_CLIENT_ID_HERE" />
    <add key="IdentityServer_ClientSecret" value="FILL_CLIENT_SECRET_HERE" />
    <add key="IdentityServer_Scope" value="cass_member_api" />

    <!-- These Member settings need to be edited to have the appropriate member-specific account -->
    <add key="Member_UserName" value="FILL_YOUR_USERNAME_HERE" />
    <add key="Member_Password" value="FILL_YOUR_PASSWORD_HERE" />
  </appSettings>
</configuration>